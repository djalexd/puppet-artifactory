<remoteRepository>
  <key><%= @name %></key>
  <description><%= @description %></description>
  <includesPattern><%= @includes %></includesPattern>
  <excludesPattern><%= @excludes %></excludesPattern>
  <repoLayoutRef><%= @layout %></repoLayoutRef>
  <enableNuGetSupport>false</enableNuGetSupport>
  <blackedOut>false</blackedOut>
  <handleReleases><%= @handle_releases %></handleReleases>
  <handleSnapshots><%= @handle_snapshots %></handleSnapshots>
  <maxUniqueSnapshots><%= @max_unique_snapshots %></maxUniqueSnapshots>
  <suppressPomConsistencyChecks><%= @suppress_pom_consistency_checks %></suppressPomConsistencyChecks>
  <propertySets/>
  <archiveBrowsingEnabled><%= @archive_browsing_enabled %></archiveBrowsingEnabled>
  <url><%= @url %></url>
  <offline><%= @offline %></offline>
  <hardFail><%= @hard_fail %></hardFail>
  <storeArtifactsLocally><%= @store_artifact_locally %></storeArtifactsLocally>
  <fetchJarsEagerly><%= @fetch_jars_eagerly %></fetchJarsEagerly>
  <fetchSourcesEagerly><%= @fetch_sources_eagerly %></fetchSourcesEagerly>
  <retrievalCachePeriodSecs><%= @retrieval_cache_period_secs %></retrievalCachePeriodSecs>
  <assumedOfflinePeriodSecs><%= @assumed_offline_period_secs %></assumedOfflinePeriodSecs>
  <missedRetrievalCachePeriodSecs><%= @missed_retrieval_cache_period_secs %></missedRetrievalCachePeriodSecs>
  <remoteRepoChecksumPolicyType><%= @remote_repo_checksum_policy_type %></remoteRepoChecksumPolicyType>
  <unusedArtifactsCleanupPeriodHours><%= @unused_artifacts_cleanup_period_hours %></unusedArtifactsCleanupPeriodHours>
  <shareConfiguration><%= @share_configuration %></shareConfiguration>
  <synchronizeProperties><%= @synchronize_properties %></synchronizeProperties>
  <listRemoteFolderItems><%= @list_remote_folder_items %></listRemoteFolderItems>
  <rejectInvalidJars><%= @reject_invalid_jars %></rejectInvalidJars>
  <p2Support><%= @p2_support %></p2Support>
  <socketTimeoutMillis><%= @socket_timeout_millis %></socketTimeoutMillis>
</remoteRepository>
